<h2><%= @page.name  %></h2>

<ul>
  <% @page.page_elements.each do |element| %>
    <li><%= element.class.class_name %></li>
  <% end %>
</ul>

<%= link_to "Edit", edit_admin_page_path(@page) %>
<% if current_admin_user.is_superadmin? %>
  <%= link_to 'Destroy', admin_page_path, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>